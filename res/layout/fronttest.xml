<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent" 
	android:background="#FFFFFFFF" 
	android:layout_weight="1" android:orientation="vertical">
	<com.google.android.maps.MapView
        android:id="@+id/mapview"
        android:layout_width="fill_parent"
        android:layout_height="300px"
        android:clickable="true"
        android:apiKey="0ZK1LP9j9AyoHeriR9RnN8bDyd5TNTpl9JBqCeg"
    />
    <ImageButton android:id="@+id/gotobookalibrarianbutton"
    	android:layout_height="64dip" 
    	android:layout_width="64dip"
    	android:background="@drawable/bbk">
    </ImageButton>
	<ImageButton android:id="@+id/gotosearchbutton" 
		android:background="@drawable/searchbook"
		android:layout_width="64dip"
		android:layout_height="64dip">
	</ImageButton>
	<ImageButton android:id="@+id/gototwitterbutton"
		android:background="@drawable/twitterb"
		android:layout_width="64dip"
		android:layout_height="64dip">
	</ImageButton>
	<ImageButton android:id="@+id/findthewaybutton" 
		android:background="@drawable/mapb"
		android:layout_width="64dip"
		android:layout_height="64dip">
	</ImageButton>
	<ImageButton android:id="@+id/gotofacebookbutton" 
		android:background="@drawable/fbb"
		android:layout_width="64dip"
		android:layout_height="64dip">
	</ImageButton>
</LinearLayout>